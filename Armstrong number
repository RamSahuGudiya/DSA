let num = 153;
const original = num;
let sum = 0;
while (num > 0) {
  let rem = num % 10;
  //   console.log(rem);
  //   console.log(num);

  sum = sum + rem * rem * rem;
  num = Math.trunc(num / 10);
  console.log(num);
}

const verified = sum === original ? 'Armstrong' : 'Not a Armstrong';

console.log(verified);
